    "def numberOfBeams(bank):\n",
    "    ans = 0\n",
    "\n",
    "    prev = 0\n",
    "    for row in bank:\n",
    "        k = row.count('1')\n",
    "        if k != 0:\n",
    "            ans += prev * k\n",
    "            prev = k\n",
    "    \n",
    "    return ans"
   ]
  }
