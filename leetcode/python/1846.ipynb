    "def maximumElementAfterDecrementingAndRearranging(arr):\n",
    "    arr.sort()\n",
    "    arr[0] = 1\n",
    "    for i in range(1, len(arr)):\n",
    "        if arr[i] - arr[i-1] > 1:\n",
    "            arr[i] = arr[i-1] + 1\n",
    "    \n",
    "    return arr[-1]"
   ]
