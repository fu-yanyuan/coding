    "def findValueOfPartition(nums):\n",
    "    nums.sort()\n",
    "    \n",
    "    minGap = float('inf')\n",
    "    for i in range(1, len(nums)):\n",
    "        minGap = min(minGap, nums[i] - nums[i-1])\n",
    "    \n",
    "    return minGap\n",
    "\n",
    "\n",
    "nums = [100,1,10]\n",
    "findValueOfPartition(nums)"
   ]
  }
